{
  "name": "mepd-frontend",
  "version": "1.0.0",
  "description": "PS Co. monitoring and evalution unit system and Co. dashboard",
  "main": "index.js",
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "install:clean": "rm -rf node_modules && npm install",
    "pretty-quick": "pretty-quick",
    "lint": "tsc --noEmit && eslint './src/**/*.{ts,tsx}' --ignore-path .eslintignore --quiet",
    "unit": "BABEL_ENV=test jest src/ --passWithNoTests -t",
    "test": "echo TODO run tests but not cypress",
    "cypress:run": "cypress run -s test/integration/**/${test_name}.spec.js",
    "cypress:open": "cypress open",
    "start": "NODE_ENV=development webpack serve --mode development",
    "build": "NODE_ENV=production webpack --mode production --progress",
    "analyze": "ANALYZE_BUNDLE=true npm run build",
    "uikit:preview": "start-storybook -p 6006"
  },
  "keywords": [
    "MEPD",
    "dashboard",
    "palestine",
    "survey",
    "data",
    "collection"
  ],
  "author": "Mohammed M. Melhem",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.17.10",
    "@babel/core": "^7.18.2",
    "@babel/plugin-proposal-class-properties": "^7.17.12",
    "@babel/plugin-proposal-object-rest-spread": "^7.18.0",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/preset-env": "^7.18.2",
    "@babel/preset-react": "^7.17.12",
    "@babel/preset-typescript": "^7.17.12",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.7",
    "@storybook/addon-actions": "^6.5.7",
    "@storybook/addon-docs": "^6.5.7",
    "@storybook/addon-essentials": "^6.5.7",
    "@storybook/addon-links": "^6.5.7",
    "@storybook/addons": "^6.5.7",
    "@storybook/builder-webpack5": "^6.5.7",
    "@storybook/manager-webpack5": "^6.5.7",
    "@storybook/react": "^6.5.7",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^14.2.0",
    "@types/jest": "^28.1.1",
    "@types/loadable__component": "^5.13.4",
    "@types/lodash": "^4.14.182",
    "@types/node": "^17.0.40",
    "@types/react": "^18.0.12",
    "@types/react-dom": "^18.0.5",
    "@types/react-router-dom": "^5.3.3",
    "@types/styled-components": "^5.1.25",
    "@typescript-eslint/eslint-plugin": "^5.27.1",
    "@typescript-eslint/parser": "^5.27.1",
    "babel-core": "^6.26.3",
    "babel-loader": "^8.2.5",
    "babel-plugin-dynamic-import-node": "^2.3.3",
    "babel-plugin-lodash": "^3.3.4",
    "babel-plugin-macros": "^3.1.0",
    "babel-plugin-styled-components": "^2.0.7",
    "babel-plugin-syntax-class-properties": "^6.13.0",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "clean-webpack-plugin": "^4.0.0",
    "core-js": "^3.22.8",
    "css-loader": "^6.7.1",
    "cypress": "^10.0.3",
    "eslint": "^8.17.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-lodash": "^7.4.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react": "^7.30.0",
    "eslint-plugin-react-hooks": "^4.5.0",
    "favicons": "^7.0.0-beta.4",
    "favicons-webpack-plugin": "^5.0.2",
    "fork-ts-checker-webpack-plugin": "^7.2.11",
    "html-webpack-plugin": "^5.5.0",
    "jest": "^28.1.1",
    "lodash-webpack-plugin": "^0.11.6",
    "mini-css-extract-plugin": "^2.6.0",
    "prettier": "^2.6.2",
    "react-refresh": "^0.13.0",
    "source-map-loader": "^3.0.1",
    "storybook-dark-mode": "^1.1.0",
    "style-loader": "^3.3.1",
    "typescript": "^4.7.3",
    "webpack": "^5.73.0",
    "webpack-bundle-analyzer": "^4.5.0",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.9.2",
    "workbox-webpack-plugin": "^6.5.3"
  },
  "dependencies": {
    "@loadable/component": "^5.15.2",
    "@sentry/browser": "^7.1.0",
    "axios": "^0.27.2",
    "date-fns": "^2.28.0",
    "lodash": "^4.17.21",
    "make-plural": "^7.1.0",
    "react": "^18.1.0",
    "react-aria": "^3.16.0",
    "react-dom": "^18.1.0",
    "react-hot-toast": "^2.2.0",
    "react-icons": "^4.4.0",
    "react-merge-refs": "^1.1.0",
    "react-router": "^6.3.0",
    "react-router-dom": "^6.3.0",
    "react-stately": "^3.14.0",
    "react-use": "^17.4.0",
    "styled-components": "^5.3.5",
    "workbox-window": "^6.5.3"
  },
  "jest": {
    "roots": [
      "<rootDir>/src"
    ],
    "transform": {
      "^.+\\.jsx?$": "babel-jest",
      "^.+\\.tsx?$": "babel-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.tsx?$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json"
    ],
    "moduleNameMapper": {
      "\\.(css|less)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(jpg|ico|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "^~constants(.*)$": "<rootDir>/src/constants$1",
      "^~services(.*)$": "<rootDir>/src/services$1",
      "^~uikit(.*)$": "<rootDir>/src/components/uikit$1",
      "^~components(.*)$": "<rootDir>/src/components$1",
      "^~utils(.*)$": "<rootDir>/src/utils$1"
    }
  },
  "prettier": {
    "bracketSpacing": true,
    "trailingComma": "es5",
    "tabWidth": 2,
    "semi": true,
    "singleQuote": true,
    "arrowParens": "avoid"
  }
}
